package test;

public class Main {
	
	public static void main(String args[]){
		
		String str1 = "hello";
		char [] array = str1.toCharArray();
		for (int i = 0; i < array.length; i++) {
			System.out.println(array[i]);
		}
		String str2 =new String(array);
		System.out.println(str2);		
	}

}
